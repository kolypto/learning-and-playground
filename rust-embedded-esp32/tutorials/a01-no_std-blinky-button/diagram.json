{
    "version": 1,
    "editor": "wokwi",
    "parts": [
        { "type": "board-esp32-c3-devkitm-1", "id": "esp", "top": 0, "left": 0, "attrs": { "flashSize": "16" } },
        { "type": "wokwi-led", "id": "led1", "top": 50, "left": 100, "attrs": { "color": "red" } },
        { "type": "wokwi-pushbutton", "id": "btn-red", "top": 50, "left": 130, "attrs": { "color": "red", "bounce": "0" } }
    ],
    "connections": [
        [ "esp:TX", "$serialMonitor:RX", "", [] ],
        [ "esp:RX", "$serialMonitor:TX", "", [] ],
        [ "esp:GND.8", "led1:C", "black", [ "v0" ] ],
        [ "led1:A", "esp:7", "green", [ "v0" ] ],
        [ "btn-red:2.l", "esp:GND.8", "black", [ "v0" ] ],
        [ "btn-red:1.l", "esp:9", "green", [ "v0" ] ]
    ],
    "serialMonitor": {
        "display": "terminal",
        "convertEol": true
    }
}
